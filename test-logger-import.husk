// utils.husk
enum LogLevel {
    Debug,
    Info,
    Warn,
    Error,
}

struct Logger {
    level: LogLevel,
}

impl Logger {
    fn new(level: LogLevel) -> Logger {
        Logger { level: level }
    }
    
    fn info(self, message: string) {
        match self.level {
            LogLevel::Debug => println(format!("[INFO] {}", message)),
            LogLevel::Info => println(format!("[INFO] {}", message)),
            LogLevel::Warn => {},
            LogLevel::Error => {}
        }
    }
}