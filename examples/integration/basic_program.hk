// Basic integration example: functions, enums, match, and expressions.

enum Result<T, E> {
    Ok(T),
    Err(E),
}

enum Color {
    Red,
    Blue,
}

fn mix(a: Color, b: Color) -> Color {
    match a {
        Color::Red => b,
        Color::Blue => a,
    }
}

fn main() {
    let c1: Color = Color::Red;
    let c2: Color = Color::Blue;
    let mixed = mix(c1, c2);
    let n: i32 = match mixed {
        Color::Red => 10,
        Color::Blue => 20,
    };
    n;
}
