async fn check_file_exists(path: string) -> Result<bool, string> {
    Result::Ok(true)
}

async fn process_files(input: string) -> Result<(), string> {
    let input_exists = check_file_exists(input.clone()).await?;
    if !input_exists {
        return Result::Err("Input path does not exist");
    }
    Result::Ok(())
}