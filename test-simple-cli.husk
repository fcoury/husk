fn println_args(args: array<string>) {
    println("Arguments:");
    for i in 0..args.len() {
        println(format!("  {}: {}", i, args[i]));
    }
}

fn main() {
    let args = ["cli", "process", "input.txt", "output.txt"];
    println_args(args);
    
    if args.len() < 2 {
        println("Error: Not enough arguments");
        return;
    }
    
    let command = args[1];
    
    if command == "process" {
        if args.len() < 4 {
            println("Error: Process command requires input and output paths");
            return;
        }
        let input = args[2];
        let output = args[3];
        println(format!("Processing from {} to {}", input, output));
    } else {
        if command == "help" {
            println("Usage: cli <command> [args]");
            println("Commands:");
            println("  process <input> <output>  - Process a file");
            println("  help                      - Show this help");
        } else {
            println(format!("Error: Unknown command: {}", command));
        }
    }
}