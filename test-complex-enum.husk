enum Status {
    Idle,
    Running { pid: int },
    Error { code: int, message: string },
}

fn main() {
    let s1 = Status::Idle;
    let s2 = Status::Running { pid: 1234 };
    let s3 = Status::Error { code: 404, message: "Not found" };
    
    match s2 {
        Status::Idle => println("Idle"),
        Status::Running { pid } => println(format!("Running with PID: {}", pid)),
        Status::Error { code, message } => println(format!("Error {}: {}", code, message))
    }
}