{
  "name": "husk-light",
  "type": "light",
  "colors": {
    "editor.background": "#faf8f5",
    "editor.foreground": "#24292f"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6e7781",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#cf222e"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "entity.name.function.macro"
      ],
      "settings": {
        "foreground": "#8250df"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#0a3069"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class",
        "entity.name.type.class",
        "storage.type.primitive"
      ],
      "settings": {
        "foreground": "#0550ae"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character"
      ],
      "settings": {
        "foreground": "#953800"
      }
    },
    {
      "scope": [
        "punctuation",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#24292f"
      }
    },
    {
      "scope": [
        "variable",
        "variable.parameter",
        "variable.other"
      ],
      "settings": {
        "foreground": "#24292f"
      }
    },
    {
      "scope": [
        "variable.language.self",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "#cf222e"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#116329"
      }
    }
  ]
}
